//function greeting using a preselected name 

//function called greeting() takes name as a parameter.
//function returns string equal to "Hi, I am" + " " + name.
//Call greeting() by passing it your name, and use console.log to print the output

var greeting = function (name) {	
	return "Hi, I am" + " " + name;
	greeting("Anna");
};

console.log(greeting("Anna"));

//prints out Hi, I am Anna
