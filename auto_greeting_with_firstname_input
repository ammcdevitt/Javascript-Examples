//function greeting using a preselected name 

//function called nameString() takes name as a parameter. fuction returns a string equal to 
//"Hi, I am" + " " + name. Call nameString() by passing it your name, and use console.log to print the output
var firstName = function (name) {	
	return "Hi, I am" + " " + name;
	firstName"Anna");
};

console.log(firstName("Anna"));

//prints out Hi, I am Anna
